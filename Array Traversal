#include<stdio.h>
void prinArray(int* a,int size)
{
    int i=0;
    printf("Enter elements of array:\n");
    for (i=0;i<size;i++){
        scanf("%d",&a[i]);
    }printf("");
    for (i=0;i<size;i++){
        printf("%d\n",a[i]);
        }
}
int main()
{
int a[]={};
int size=5;
prinArray(a, size);

return 0;
}
